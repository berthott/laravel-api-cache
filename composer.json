{
  "name": "berthott/laravel-api-cache",
  "description": "Laravel Helper for Caching API Responses",
  "license": "MIT",
  "authors": [
      {
          "name": "Jan Bladt",
          "email": "berthott@web.de"
      }
  ],
  "autoload" : {
      "psr-4" : {
          "berthott\\ApiCache\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "berthott\\ApiCache\\Tests\\": "tests"
      }
  },
  "scripts": {
        "test": "vendor/bin/phpunit"
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "berthott\\ApiCache\\ApiCacheServiceProvider"
          ]
      }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  }
}
